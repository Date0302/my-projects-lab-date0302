{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "S3ReadOriginalBucket",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject"
      ],
      "Resource": "arn:aws:s3:::my-unique-image-upload-bucket/*"
    },
    {
      "Sid": "S3WriteProcessedBucket",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject"
      ],
      "Resource": "arn:aws:s3:::my-image-processed-first/*"
    }
  ]
}